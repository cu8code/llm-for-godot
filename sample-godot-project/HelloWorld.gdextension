[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../extension/target/debug/librust.so"
linux.release.x86_64 =   "res://../extension/target/release/librust.so"
windows.debug.x86_64 =   "res://../extension/target/debug/rust.dll"
windows.release.x86_64 = "res://../extension/target/release/rust.dll"
macos.debug =            "res://../extension/target/debug/librust.dylib"
macos.release =          "res://../extension/target/release/librust.dylib"
macos.debug.arm64 =      "res://../extension/target/debug/librust.dylib"
macos.release.arm64 =    "res://../extension/target/release/librust.dylib"
